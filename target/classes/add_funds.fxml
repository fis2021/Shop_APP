   <children>
      <Button layoutX="124.0" layoutY="70.0" mnemonicParsing="false" onAction="#updateFunds" text="Confirm" />
      <Button layoutX="190.0" layoutY="70.0" mnemonicParsing="false" onAction="#handleClose" text="Cancel" />
      <TextField fx:id="funds" layoutX="91.0" layoutY="35.0" promptText="Insert number here." />
      <Text layoutX="39.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Funds" />
   </children>
</AnchorPane>
